{
  "scripts": {
    "cy:run": "cypress run",
    "cy:allure": "cypress run --env allure=true", 
    "cy:allure:default": "npx cypress run --env allure=true --config-file cypress.config.js --browser chrome",
    "cy:allure:mobile": "npx cypress run --env allure=true --config-file cypress.config.mobile.js --browser chrome",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "report": "npm run allure:generate && npm run allure:open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.34.1",
    "cypress": "^15.5.0"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.2.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.6"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step_definitions/*.js"
  }
}
